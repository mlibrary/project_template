# This file is part of the machinery needed to build against multiple database
# backends in travis for rails apps. It's not necessary if you're not doing that.
# Most projects will likely not need to support multiple databases.

test:
  adapter: mysql2
  encoding: utf8
  pool: 5
  timeout: 5000
  reconnect: true
  port: 3306
  database: somedb
  username: travis

